{
  "name": "@manifoldco/gql-zero",
  "version": "0.1.0",
  "description": "A lightweight alternative for consuming GraphQL. Zero caching, zero bells. Zero Bloat.",
  "scripts": {
    "test": "jest",
    "build": "pack build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifoldco/gql-zero.git"
  },
  "keywords": [
    "graphql",
    "gql",
    "esm",
    "es modules",
    "react",
    "lightweight",
    "zero"
  ],
  "author": "@manifoldco",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/manifoldco/gql-zero/issues"
  },
  "homepage": "https://github.com/manifoldco/gql-zero#readme",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "**/*.test.ts"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.3.7",
    "@pika/plugin-build-node": "^0.3.16",
    "@pika/plugin-build-types": "^0.3.16",
    "@pika/plugin-build-web": "^0.5.1",
    "@pika/plugin-standard-pkg": "^0.3.16",
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  }
}
